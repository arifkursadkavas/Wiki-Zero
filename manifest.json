{
  "manifest_version": 2,
  "name": "Wiki Zero",
  "description": "Knowledge conquers the universe",
  "version": "1.2",

    "permissions": [ "tabs", "notifications", "unlimitedStorage", "webRequest", "webRequestBlocking", "<all_urls>"],

  "background": {
    "page": "background.html"
  },

  "browser_action": {
   "default_icon": "img/wiki_logo.png"
  },

  "icons": { "16": "img/wiki_logo.png",
           "48": "img/wiki_logo.png",
          "128": "img/wiki_logo.png" },
  "content_security_policy": "script-src 'self' https://www.google-analytics.com; object-src 'self'"
}
